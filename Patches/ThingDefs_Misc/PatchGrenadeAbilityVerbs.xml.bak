<?xml version="1.0" encoding="utf-8" ?>
<Patch>
 <!-- ====================== Grenades - Imperial ===================== -->	
 <!-- ====================== OGI_Ability_ThrowFrag ===================== -->	
	<Operation Class="PatchOperationSequence">
	<success>Normal</success>
		<operations>	
			<li Class="PatchOperationTest">
				<xpath>/Defs/AbilityUser.AbilityDef[defName = "OGI_Ability_ThrowFrag"]</xpath>

			</li>
			<!--
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="Projectile_OGIGrenadeFrag"]</xpath>
				<value>
			<thingClass>CombatExtended.ProjectileCE_Explosive</thingClass>
			</value>
			</li>
			<li Class="PatchOperationAttributeSet">
				<xpath>Defs/ThingDef[defName="Projectile_OGIGrenadeFrag"]/projectile</xpath>
				<attribute>Class</attribute>
				<value>CombatExtended.ProjectilePropertiesCE</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName="Projectile_OGIGrenadeFrag"]/projectile</xpath>
				<value><applyDamageToExplosionCellsNeighbors>true</applyDamageToExplosionCellsNeighbors></value>
			</li>
			-->
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="Projectile_OGIGrenadeFrag"]</xpath>
				<value>
					<comps>
						<li Class="CombatExtended.CompProperties_ExplosiveCE">
							<fragments>
								<Fragment_GrenadeFrag>105</Fragment_GrenadeFrag>
							</fragments>
						</li>
					</comps>
			</value>
			</li>
		</operations>
	</Operation>

